function AIML()
gg.clearResults()
gg.setRanges(gg.REGION_...Type Region....)
gg.searchNumber("...Value dan grub search...", gg.REGION_....Type Region...)
gg.refineNumber("...Value yg mau kmu cari...", gg.REGION_....Type Region...)
gg.getResults(...jumlah hasil...)
gg.editAll("...Value yg mau kmu edit...", gg.REGION_....Type Region...)
gg.clearResults()
gg.toast("️.....nama fitur....")
gg.sleep(800)
gg.toast("Ⓑⓨ ➠ ★᭄ꦿ᭄ꦿ.....Kasih nama kalian.....★᭄ꦿ᭄ꦿ")
gg.sleep(800)
end

function AIMG()
AH = gg.multiChoice({
"✪ • Edit Menu Fitur 1",
"✪ • Edit Menu Fitur 2",
"🔙 ʙᴀᴄᴋ"
}, nil, os.date("*╔═══════════❖•ೋ° °ೋ•❖═══════════╗*\n ❦🆈🅾🆄🆃🆄🅱🅴 🅲🅷🅰🅽🅽🅴🅻 : 🅱🅰🅽🅶 🆃🅰🅼🅰❦\n 🄳🄾🄽🅃 🄵??🅁🄶🄴🅃 🅃🄾 🅂🅄🄱🅂🄲🅁🄸🄱🄴 🄱🅁🄾\n             ⏱️ Today : %Y/%m/%d Time: %H:%M:%S ⏱️\n ━─━───────────༺ɢᴀᴍᴇ༻──────────━─━"))
if AH == nil then
else
if AH[1] == true then
AIMG1()
end
if AH[2] == true then
AIMG2()
end
if AH[3] == true then
HOME()
end
end
TAMA = -1
end

function AIMG1()
gg.clearResults()
gg.setRanges(gg.REGION_...Type Region....)
gg.searchNumber("...Value dan grub search...", gg.REGION_...Type Region...)
gg.refineNumber("...Value yg mau kmu cari...", gg.REGION_...Type Region....)
gg.getResults(...jumlah hasil value..)
gg.editAll("...Value yg mau kmu edit...", gg.REGION_...Type Region....)
gg.clearResults()
gg.toast("️.....nama fitur....")
gg.sleep(800)
gg.toast("Ⓑⓨ ➠ ★᭄ꦿ᭄ꦿ.....Kasih nama kalian.....★᭄ꦿ᭄ꦿ")
gg.sleep(800)
end

