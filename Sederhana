function EXIT()
EXP =   gg.alert("*╔═══════════❖•ೋ° °ೋ•❖═══════════╗*\n ❦🆈🅾🆄🆃🆄🅱🅴 🅲🅷🅰🅽🅽🅴🅻 : 🅱🅰🅽🅶 🆃🅰🅼🅰❦\n 🄳🄾🄽🅃 🄵🄾🅁🄶🄴🅃 🅃🄾 🅂🅄🄱🅂🄲🅁🄸🄱🄴 🄱🅁🄾\n ━─━───────────༺ᴇxɪᴛ༻──────────━─━","EXIT","BACK","COPY LINK YOUTUBE")
if EXP == 1 then
EX()
end
if EXP == 2 then
HOME()
end
if EXP == 3 then
COPY()
end
end

function COPY()
  gg.copyText("https://www.youtube.com/BangTAMA", false)
  gg.toast("🅨🅞🅤🅣🅤🅑🅔 ➧ 🅑🅐🅝🅖 🅣🅐🅜🅐")
end 

function EX()
  gg.toast("🦇sᴄʀɪᴘᴛ ᴇxɪᴛ🦇")
print(os.date("🦇sᴄʀɪᴘᴛ ᴇxɪᴛ🦇\n⏱️ Today : %Y/%m/%d Time: %H:%M:%S ⏱️\n🄼🅈 🄽🅄🄼🄱🄴🅁 : 𝟎𝟖𝟐𝟐********"))
print("𝕐𝕆𝕌𝕋𝕌𝔹𝔼 ℂℍ𝔸ℕℕ𝔼𝕃  : ★᭄ꦿ᭄ꦿᗷᗩᑎᏀ Ꭲᗩᗰᗩ★᭄ꦿ᭄ꦿ\n𝔽ℝ𝕆𝕄 𝕀ℕ𝔻𝕆ℕ𝔼𝕊𝕀𝔸 🇮🇩  \n➡ 𝐋𝐄𝐆𝐄𝐍𝐃 𝐒𝐐𝐔𝐀𝐃 𝐆𝐀𝐙𝐀 🇮🇩🇮🇩\n\n        ➧ Ⓢⓒⓡⓘⓟⓣ Ⓑⓨ ➠\n\n                    ★᭄ꦿ᭄ꦿㄒ卂爪卂★᭄ꦿ᭄ꦿ\n")
  gg.skipRestoreState()
  gg.setVisible(true)
os.exit()
HOME()
end

while true do
if gg.isVisible(true) then
TAMA = 1
gg.setVisible(false)
end
gg.clearResults()
if TAMA == 1 then
HOME()
end
end
